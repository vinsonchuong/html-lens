{
  "name": "html-lens",
  "version": "0.0.1",
  "description": "An interface for reading and transforming HTML",
  "keywords": [
    "html",
    "parser"
  ],
  "homepage": "https://github.com/vinsonchuong/html-lens",
  "bugs": "https://github.com/vinsonchuong/html-lens/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "src/HtmlLens.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsonchuong/html-lens"
  },
  "scripts": {
    "test": "standard --fix && flow && ava",
    "prepare": "build-esm"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "cssauron": "^1.4.0",
    "minidom": "^1.0.0",
    "pretty": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-plugin-package-name-import": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "build-esm": "^1.1.3",
    "dedent": "^0.7.0",
    "flow-bin": "^0.39.0",
    "standard-esnext": "^1.0.3"
  },
  "babel": {
    "presets": [
      "latest",
      "flow"
    ],
    "plugins": [
      "transform-runtime",
      "package-name-import"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
